// Online C compiler to run C program online
#include <stdio.h>
#include <string.h>
int main() {
    // Write C code here
    char nome[10];
    printf("Digite o nome: \n"); 
    fgets(nome, 10, stdin);
    char sobrenome[15];
    printf("Digite o sobrenome: \n"); 
    fgets(sobrenome, 15, stdin);
    
    return 0;
}
