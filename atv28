// Online C compiler to run C program online
#include <stdio.h>
    
    int main(){
    int x, y;
    printf("Informe dois números positivos e inteiros: "); scanf("%d%d", &x, &y);
    if(x > y){
        printf("Este número é maior %d", x);
    }else
    if(x < y){
       printf("Este número é maior %d", y); 
    }else
    if(x == y){
        printf("São iguais");
    }
    return 0;
}
