// Online C compiler to run C program online
#include <stdio.h>
    
    int main(){
    int n;
    printf("Informe um numero de 0 a 100: "); scanf("%d", &n);
    
    if(n <= 49){
        printf("Nota de 0 a 49 é Insuficiente");
    }else
    if(n >= 50 && n <= 64){
        printf("Nota de 50 a 64 é Regular");
    }else
    if (n >= 65 && n <= 84){
        printf("Nota de 65 a 84 é Bom");
    }else
    if(n >= 85 && n <= 100){
        printf("Nota de 85 a 100 é Ótimo");
    }else{
        printf("Numero invalido");
    }
    return 0;
}
