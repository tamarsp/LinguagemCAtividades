// Online C compiler to run C program online
#include <stdio.h>
    
    int main(){
    int a, b, media;
    printf("Informe sua nota dos bimestres: "); scanf("%d%d", &a, &b);
    
    media = (a + b)/2;
    printf("A média dos dois bimestre é %d \n", media);
    if(media >= 5)
    printf("\nAprovado");
    else
    printf("\nReprovado");
    return 0;
}
