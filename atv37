// Online C compiler to run C program online
#include <stdio.h>

    int main(){
    int opcao;
    printf("Opção 1) Idades De 05 a 10;\n Opção 2)  De 11 a 15; \n Opção 3) De 16 a 20;\n Opção 4) De 21 a 25;\n");
    printf("\n\nInforme um número de 1 a 4: "); scanf("%d", &opcao);
    
    switch(opcao)
    {
    case 1 :
        printf("Infantil");
    break;
    case 2 :
        printf("Juvenil");
    break;
    case 3 :
        printf("Junior");
    break;
    case 4 :
        printf("Profissional");
    break;
    
    default:
    printf("Numero invalido");
    }
   
    return 0;
}
