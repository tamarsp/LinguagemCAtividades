// Online C compiler to run C program online
#include <stdio.h>

int main() {
    int a, b, c;
    printf("Digite 2 numeros inteiros: "); scanf("%d%d", &a, &b);
    printf("Digite um numero: "); scanf("%d", &c);
    a = b * c;
    if(a % c == 0)
        printf("è multiplo");
    else
    printf("Não é multiplo");

    return 0;
}
