#include <stdio.h>

int main() {
    float numero;
    int contador = 0;

    printf("Informe um número: ");
    scanf("%f", &numero);

    while (numero >= 1) {
        numero = numero / 2;
        contador++;
    }

    printf("\nUltima divisão: %.6f\n", numero);
    printf("Quantidade de divisões: %d\n", contador);

    return 0;
}
