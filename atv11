// Online C compiler to run C program online
#include <stdio.h>

int main() {
    float comprimento, altura, largura, volume;
    printf("Digite o comprimento: "); scanf("%f", &comprimento);
    printf("Digite a altura: "); scanf("%f", &altura);
    printf("Digite a largura: "); scanf("%f", &largura);
    
    volume = comprimento * altura * largura;
    printf("Resultado: %f", volume);
    return 0;
}

