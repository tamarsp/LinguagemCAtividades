// Online C compiler to run C program online
#include <stdio.h>

int main() {
   int x, y, z;
   printf("Informe as medidas do triangulo: "); scanf("%d%d%d", &x, &y, &z);

   if(x == y && y == z){
        printf("Triângulo Equilátero");
   }else
   if(x == y && x != z && y != z){
        printf("Triângulo Isósceles");    
   }else
   if(x != y && x != z && y != z){
        printf("Triângulo Escaleno");       
   }else{
        printf("Não é um triângulo");
   }
    return 0;
}
