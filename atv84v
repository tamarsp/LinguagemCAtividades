// Online C compiler to run C program online
#include <stdio.h>
int main() {
    int x, y,z;
    printf("Informe uma centena: ");
    scanf("%d", &x);
    printf("Informe uma dezena: ");
    scanf("%d", &y);
    printf("Informe uma unidade: ");
    scanf("%d", &z);
    printf("Centena: %d\n Dezena: %d\n Unidade: %d", x, y, z);
    return 0;
}
