#include <stdio.h>

int main() {
    int a = 0;
    int b = 1;
    int resultado;
    printf("%d, %d", a, b);
     for(int c = 3; c <= 20; c++){
         resultado = a + b;
          printf(" \nresultado %d\n", resultado);
         a = b;
         b = resultado;
    }
    printf("\n");
    return 0;
}
